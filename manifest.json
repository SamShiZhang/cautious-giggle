{
  "manifest_version": 3,
  "name": "Keyword Highlighter",
  "version": "1.2",
  "description": "A simple keyword highlighter",
  "permissions": ["storage", "activeTab"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
  
}
